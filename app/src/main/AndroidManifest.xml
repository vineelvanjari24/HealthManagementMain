        <activity
            android:name=".UserLogin.Login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DoctorList.DoctorActivity"
            android:exported="false" />
        <activity
            android:name=".TrackYourHealth.Fragment.Ailment"
            android:exported="false" />
        <activity
            android:name=".UserHealth.AddUserHealthActivity"
            android:exported="false" />
